{
  "manifest_version": 2,
  "name": "Github Repositories Search",
  "description": "Search Github repository by name, topic, username or readme",
  "version": "0.0.5",
  "author":"Faraj Khasib",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
   "content_security_policy": "script-src 'self'; object-src 'self'",
  "icons": {
    "16": "favicon-16x16.png",
    "48": "favicon-144x144.png",
    "128": "favicon-144x144.png"
  },
  "permissions": [
  ]
}
